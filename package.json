    "gulp-autoprefixer": "~3.1.0",
    "gulp-concat": "~2.6.0",
    "gulp-hogan": "^2.0.0",
    "gulp-insert": "~0.5.0",
    "gulp-remove-code": "^1.0.2",
    "gulp-rename": "^1.4.0",
